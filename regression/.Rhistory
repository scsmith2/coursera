library(UsingR); data(diamond)
y <- diamond$price; x<- diamond$carat; n <- length(y)
fit <- lm(y~x)
e <- resid(fit)
max(abs(e - (y - yhat)))
max(abs(e - (y - coef(fit)[1] - coef(fit)[2] * x)))
plot(x,e,
xlab="Mass (Carats)",
ylab = "Residuals (SIN $)")
